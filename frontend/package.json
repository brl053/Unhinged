{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development --host 0.0.0.0 --port 3000 --hot",
    "start": "webpack serve --open",
    "build": "webpack --mode production",
    "build:staging": "webpack --mode production --env staging",
    "analyze": "webpack --mode production --env analyze",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:integration:services": "TEST_INTEGRATION=true jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:diagnostic": "playwright test sidebar-navigation-diagnostic --headed",
    "test:audio": "playwright test audio-pipeline --headed",
    "test:audio-demo": "node test-audio-simple.js",
    "test:audio-custom": "npx ts-node tests/audio-pipeline-demo.ts --custom",
    "test:audio-benchmark": "npx ts-node tests/audio-pipeline-demo.ts --benchmark",
    "test:audio-all": "npx ts-node tests/audio-pipeline-demo.ts --all",
    "test:ci": "npm run test:unit:coverage && npm run test:integration:services && npm run test:e2e",
    "test:quick": "node scripts/test-runner.js --quick",
    "test:all": "node scripts/test-runner.js",
    "test:runner": "node scripts/test-runner.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.66.0",
    "@wmik/use-media-recorder": "^1.6.5-beta.0",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "monaco-editor": "^0.53.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.1.5",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/styled-components": "^5.1.34",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
