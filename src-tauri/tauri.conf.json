    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "Professional gRPC client with localhost service discovery",
    "longDescription": "A powerful gRPC client tool with server reflection support, automatic localhost service discovery, and comprehensive testing capabilities for local development."
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
