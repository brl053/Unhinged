{
  "productName": "gRPC Tool",
  "version": "0.1.0",
  "identifier": "com.grpc.tool",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../frontend/public"
  },
  "app": {
    "windows": [
      {
        "title": "gRPC Tool - Localhost Service Discovery",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false
      }
